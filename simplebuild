		    SamsungTV)		IFS="$orgIFS";menu "samsungtv";;
		    SH_4)		IFS="$orgIFS";menu "sh_4";;
		    SH4)		IFS="$orgIFS";menu "sh4";;
		    Syn_ATOM)		IFS="$orgIFS";menu "syn_atom";;
		    DOCKSTAR)		IFS="$orgIFS";menu "dockstar";;
